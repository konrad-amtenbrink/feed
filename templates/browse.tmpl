<!DOCTYPE html>
<html lang="en">
    <head>
        <title>feed</title>

        <script src="/static/js/browse.js"></script>
        <link rel="stylesheet" href="/static/styles/styles.css" />
        <script src="https://unpkg.com/flowbite@1.5.2/dist/datepicker.js"></script>
        <link
            rel="stylesheet"
            href="https://unpkg.com/flowbite@1.5.2/dist/flowbite.min.css"
        />
        <script src="/static/js/upload.js"></script>
        <script src="https://unpkg.com/flowbite@1.5.2/dist/flowbite.js"></script>
        <script>
            // On page load or when changing themes, best to add inline in `head` to avoid FOUC
            if (
                localStorage.getItem('color-theme') === 'dark' ||
                (!('color-theme' in localStorage) &&
                    window.matchMedia('(prefers-color-scheme: dark)').matches)
            ) {
                document.documentElement.classList.add('dark');
            } else {
                document.documentElement.classList.remove('dark');
            }
        </script>
    </head>
    <body class="bg-white dark:bg-gray-900 m-10" onload="onLoad()">
        <h2 class="mb-2 text-lg font-semibold text-gray-900 dark:text-white">All files:</h2>
        <ul id="files" class="max-w-md space-y-1 text-gray-500 list-disc list-inside dark:text-gray-400">
        </ul>
    </body>
</html>